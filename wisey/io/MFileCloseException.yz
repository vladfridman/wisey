package wisey.io;

import wisey.lang.IException;

/**
 * Exception that is thrown by wisey.io.CFile when trying to close a file without it being opened
 */
model MFileCloseException implements IException {

  public void report() override {
    callstack.reportException(this);
    printerr("Details: Trying to close a file without opening it first\n"); 
  }
}