package wisey.io;

import wisey.lang.IException;
import wisey.lang.MString;

/**
 * Exception that is thrown by wisey.io.CFile it is not able to close a file
 */
model MFileCouldNotCloseException implements IException {
  MString mFileName;

  public string getName() override {
    return getTypeName();
  }

  public MString getFileName() {
    return mFileName;
  }
}