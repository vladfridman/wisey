package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

model MCar {
  public int getYear() {
    return 2018;
  }
}

controller CController {
  receive MCar[]* mArray;


  public void init(MCar car) {
    mArray[4] = car;
  }

  public int getCarYear() {
    return mArray[4].getYear();
  }
}

controller CProgram implements IProgram {

  public int run(immutable string[] arguments) override {
    MCar* car = build(MCar).onHeap();
    CController* mycontroller = injector(CController).withArray(new MCar[5]).inject();
    mycontroller.init(car);

    return mycontroller.getCarYear();
  }
}

bind(IProgram).to(CProgram);

