model Color {
  double color;
  int intensity;
}

model Shape {
  int width;
  long height;
  float depth;
  Color color;
}

int main() {
  Shape rectangle = builder(Shape)
    .withWidth(3)
    .withHeight(5l)
    .withDepth(2.0)
    .withColor(builder(Color).withColor(6.7d).withIntensity(7).build())
    .build();

  return rectangle.color.intensity;
}
