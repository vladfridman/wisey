package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

controller CService scopedto wisey.threads.IThread implements IMyInterface {

  public int getValue() override {
    return 3;
  }
}

interface IMyInterface {
  int getValue();
}

controller CProgram implements IProgram {
  inject IMyInterface mInjected;

  public int run(immutable string[] arguments) override {
    return mInjected.getValue();
  }
}

bind(IProgram).to(CProgram);
bind(IMyInterface).to(CService);
