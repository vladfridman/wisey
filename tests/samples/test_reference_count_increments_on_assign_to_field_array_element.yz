package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

model MCar {
  public int getYear() {
    return 2018;
  }
}

controller CController {
  state MCar[5] mArray;

  public void setValue(MCar value) {
    mArray[4] = value;
  }

  public MCar getValue() {
    return mArray[4];
  }
}

controller CProgram implements IProgram {

  public int run() {
    MCar* car = builder(MCar).build();
    CController* mycontroller = injector(CController).inject();
    mycontroller.setValue(car);
    int result = mycontroller.getValue().getYear();
    car = null;
    return result;
  }
}

bind(CProgram).to(IProgram);
