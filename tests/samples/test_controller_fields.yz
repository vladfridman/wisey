interface IProgram {
  int run();
}

controller CService {
  receive int mNumber;

  public int getSomething() {
    return mNumber;
  }
}

controller CProgram implements IProgram {
  inject CService mService(5);

  public int run() {
    return mService.getSomething();
  }
}

int main() {
  CProgram program = inject(CProgram);

  return program.run();
}