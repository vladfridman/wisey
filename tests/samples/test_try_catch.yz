interface IProgram {
  int run();
}

model MException {
  int mValue;

  public int getValue() {
    return mValue;
  }
}

controller CThrower {
  public int doThrow() throws MException {
    MException exception = builder(MException).withValue(255).build();
    throw exception;
  }
}

controller CCatcher {
  inject CThrower mThrower;

  public int doTryCatch() {
    int value = 5;
    try {
      mThrower.doThrow();
    } catch (MException exception) {
      value = exception.getValue();
    }
    return value;
  }
}

controller CProgram implements IProgram {
  inject CCatcher mCatcher;

  public int run() {
    return mCatcher.doTryCatch();
  }
}

int main() {
  CProgram program = inject(CProgram);

  return program.run();
}
