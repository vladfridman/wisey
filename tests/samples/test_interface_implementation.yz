interface IObject {
  int getArea();
}

interface IShape {
  int getColor(int a);
}

model MSquare implements IShape, IObject {
  int mWidth;

  public int getArea() {
    return mWidth * mWidth;
  }

  public int getColor(int a) {
    return a;
  }
}

public int main() {
  MSquare square = builder(MSquare).withWidth(5).build();

  return square.getArea();
}