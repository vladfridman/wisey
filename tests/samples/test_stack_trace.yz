package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;
import wisey.lang.CThread;

controller CProgram implements IProgram {
  inject CThread* mThread;

  public int run() {
    mThread.setObjectAndMethod("object1", "method1");
    mThread.pushStack("FileA", 123);
    mThread.setObjectAndMethod("object2", "method2");
    mThread.pushStack("FileB", 57);
    mThread.dumpStack();

    mThread.popStack();
    mThread.dumpStack();

    mThread.popStack();
    mThread.dumpStack();

    mThread.popStack();
    mThread.dumpStack();
    
    mThread.setObjectAndMethod("object3", "method3");
    mThread.pushStack("FileC", 99);
    mThread.dumpStack();
    return 5;
  }
}

bind(CProgram).to(IProgram);
