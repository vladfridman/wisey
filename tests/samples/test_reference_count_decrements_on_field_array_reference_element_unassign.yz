package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

model MCar {
  public int getYear() {
    return 2018;
  }
}

controller CController {
  receive MCar[] mArray;

  public void set(MCar car) {
    mArray[4] = car;
  }

  public int getCarYear() {
    return mArray[4].getYear();
  }
}

controller CProgram implements IProgram {

  public int run() {
    MCar* car = builder(MCar).build();
    MCar[]* array = new MCar[5];
    CController* mycontroller = injector(CController).withArray(array).inject();
    mycontroller.set(car);
    mycontroller.set(null);
    car = null;

    return 5;
  }
}

bind(IProgram).to(CProgram);

