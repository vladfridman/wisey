package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

model MColor {
  int mValue;

  public int getValue() {
    return mValue;
  }
}

controller CProgram implements IProgram {

  public int run() {
    MColor* color = builder(MColor).withValue(7).build();

    return color.getValue();
  }
}

bind(CProgram).to(IProgram);
