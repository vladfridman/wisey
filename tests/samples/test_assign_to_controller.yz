package systems.vos.wisey.compiler.tests;

controller CAdder {
  public int get() { return 1; }
}

controller CProgram implements IProgram {
  inject CAdder* mAdder1;
  inject CAdder* mAdder2;

  public int run() {
    mAdder1 = mAdder2;
    return mAdder1.get() + mAdder2.get();
  }
}

bind(CProgram).to(IProgram);
