package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

model MIntNumber {
  int mNumber;

  public int getNumber() {
    return mNumber;
  }
}

controller CService {

  receive MIntNumber* mIntNumber;

  public MIntNumber getIntNumber() {
    return mIntNumber;
  }
}

controller CProgram implements IProgram {
  inject CService* mService.withIntNumber(builder(MIntNumber).withNumber(5).build());

  public long run(immutable string[] arguments) override {
    return mService.getIntNumber().getNumber();
  }
}

bind(IProgram).to(CProgram);
