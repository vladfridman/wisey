package systems.vos.wisey.compiler.tests;

import wisey.io.CFile;
import wisey.io.MFileCouldNotOpenException;
import wisey.lang.CString;
import wisey.lang.IProgram;

controller CProgram implements IProgram {
  inject CFile* mFile;

  public long run(immutable string[] arguments) override {
    try {
      mFile.openForWriting(CString.createFromString("nonexisting/test.txt"));
    } catch (MFileCouldNotOpenException* exception) {
      return 5;
    }
    return 1;
  }
}

bind(IProgram).to(CProgram);
