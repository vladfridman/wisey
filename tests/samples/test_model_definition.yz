model Color {
  double color;
}

model Shape {
  int width;
  long height;
  float depth;
  Color color;
}

public int main() {
  Shape rectangle = builder(Shape)
    .withWidth(3)
    .withHeight(5l)
    .withDepth(2.0)
    .withColor(builder(Color).withColor(6.7d).build())
    .build();

  return 0;
}
