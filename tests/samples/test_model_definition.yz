model MColor {
  double mColor;
}

model MShape {
  int mWidth;
  long mHeight;
  float mDepth;
  MColor mColor;
}

public int main() {
  MShape rectangle = builder(MShape)
    .withWidth(3)
    .withHeight(5l)
    .withDepth(2.0)
    .withColor(builder(MColor).withColor(6.7d).build())
    .build();

  return 0;
}
