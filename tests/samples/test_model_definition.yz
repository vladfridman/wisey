model MColor {
  double color;
}

model MShape {
  int width;
  long height;
  float depth;
  MColor color;
}

public int main() {
  MShape rectangle = builder(MShape)
    .withWidth(3)
    .withHeight(5l)
    .withDepth(2.0)
    .withColor(builder(MColor).withColor(6.7d).build())
    .build();

  return 0;
}
