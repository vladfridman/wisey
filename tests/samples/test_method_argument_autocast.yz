package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

model MColor {
  boolean mValue;

  public int getValue() {
    return mValue;
  }
}

controller CProgram implements IProgram {

  public int run() {
    MColor* color = builder(MColor).withValue(true).build();

    return color.getValue();
  }
}

bind(IProgram).to(CProgram);
