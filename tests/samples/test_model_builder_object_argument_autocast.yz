package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

interface IHasDirection {

}

interface IHasValue {
  int getValue();
}

model MYear implements IHasDirection, IHasValue {
  int mValue;

  public int getValue() override {
    return mValue;
  }
}

model MCar {
  IHasValue* mYear;

  public int getYear() {
    return mYear.getValue();
  }
}

controller CProgram implements IProgram {

  public long run(immutable string[] arguments) override {
    MCar* car = builder(MCar).withYear(builder(MYear).withValue(2017).build()).build();
    
    return car.getYear();
  }
}

bind(IProgram).to(CProgram);
