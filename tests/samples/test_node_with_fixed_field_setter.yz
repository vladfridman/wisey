package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

node NCar {
  int mYear;

  public void setYear(int year) {
    mYear = year;
  }

  public int getYear() {
    return mYear;
  }
}

controller CProgram implements IProgram {

  public int run() {
    NCar* car = builder(NCar).withYear(2017).build();
    
    return car.getYear();
  }
}

bind(IProgram).to(CProgram);
