package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

model MCar {
  public int getYear() {
    return 2018;
  }
}

controller CController {
  receive MCar[]* mOwnerArray;

  public void getRce() {
    MCar[] reference = mOwnerArray;
    mOwnerArray = null;
  }
}

controller CProgram implements IProgram {

  public long run(immutable string[] arguments) override {
    CController* mycontroller = injector(CController).withOwnerArray(new MCar[5]).inject();

    mycontroller.getRce();

    return 1;
  }
}

bind(IProgram).to(CProgram);

