package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

interface IShape {
  public static int getValue(MValueContainer valueContainer) {
    return valueContainer.getValue();
  }
}

model MValueContainer {
  int mValue;

  public int getValue() {
    return mValue;
  }
}

controller CProgram implements IProgram {

  public int run(immutable string[] arguments) override {
    MValueContainer* valueContainer = builder(MValueContainer).withValue(7).build();
    return IShape.getValue(valueContainer);
  }
} 

bind(IProgram).to(CProgram);
