package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;


controller CReferenceHolder {
  state int[] mArrayReference;

  public void setReference(int[] array) {
    mArrayReference = array;
  }
}

model MMyModel {
  int[][]* mArray;

  public int[][]* getArray() {
    return mArray;
  }

  public void takeReference(CReferenceHolder holder) {
    holder.setReference(mArray[3]);
  }
}

controller CProgram implements IProgram {
  inject CReferenceHolder* mReferenceHolder;

  public int run() override {
    int[][]* array = new int[4][4];
    array[3][3] = 5;
    MMyModel* mymodel = builder(MMyModel).withArray(array).build();
    mymodel.takeReference(mReferenceHolder);
    return mymodel.getArray()[3][3];
  }
}

bind(IProgram).to(CProgram);

