package systems.vos.wisey.compiler.tests;

import wisey.lang.CString;
import wisey.lang.IProgram;
import wisey.lang.MString;

controller CProgram implements IProgram {

  public int run(immutable string[] arguments) override {
    CString* cstring = CString.createFromString("123");
    printout("Before cstring: " + cstring + " capacity: " + cstring.getCapacity() + "\n");
    MString* appnedix = MString.createFromString("4567");
    cstring.appendMString(appnedix);
    printout("After cstring: " + cstring + " capacity: " + cstring.getCapacity() + "\n");
    return (int) cstring.getLength();
  }
}

bind(IProgram).to(CProgram);
