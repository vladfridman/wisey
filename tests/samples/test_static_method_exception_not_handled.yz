package systems.vos.wisey.compiler.tests;

import wisey.lang.IException;
import wisey.lang.IProgram;

model MException implements IException {
  int mValue;

  public void report() override {
  }
}

controller CThrower {
  public static int doThrow() throws MException {
    MException* exception = builder(MException).withValue(3).build();
    throw exception;
  }
}

controller CCatcher {
  public static int doSomething() {
    return CThrower.doThrow();
  }
}

controller CProgram implements IProgram {
  public int run(immutable string[] arguments) override {
    return CCatcher.doSomething();
  }
}

bind(IProgram).to(CProgram);
