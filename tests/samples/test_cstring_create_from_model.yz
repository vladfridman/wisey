package systems.vos.wisey.compiler.tests;

import wisey.lang.CString;
import wisey.lang.IProgram;
import wisey.lang.MString;

controller CProgram implements IProgram {

  public int run(immutable string[] arguments) override {
    MString* mstring = MString.createFromString("123");
    CString* cstring = CString.createFromModel(mstring);
    printout("mstring: " + mstring + " length: " + mstring.getLength() + "\n");
    printout("cstring: " + cstring + " length: " + cstring.getLength() + " capacity: " + cstring.getCapacity() + "\n");
    return (int) mstring.getLength();
  }
}

bind(IProgram).to(CProgram);
