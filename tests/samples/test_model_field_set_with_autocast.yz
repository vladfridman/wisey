package systems.vos.wisey.compiler.tests;

model MColor {
  int mValue;

  public void setValue(boolean a) {
    mValue = a;
  }

  public int getValue() {
    return mValue;
  }
}

controller CProgram implements IProgram {

  public int run() {
    MColor color = builder(MColor).withValue(5).build();
    
    color.setValue(true);

    return color.getValue();
  }
}

bind(CProgram).to(IProgram);
