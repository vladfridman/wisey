package systems.vos.wisey.compiler.tests;

import wisey.lang.IProgram;

model MCar {
  public int getYear() {
    return 2018;
  }
}

controller CController {
  state MCar[5] mArray;

  public void setValue(MCar value) {
    mArray[4] = value;
  }

  public MCar getValue() {
    return mArray[4];
  }

  public void clearValue() {
    mArray[4] = null;
  }
}

controller CProgram implements IProgram {

  inject CController* mController;

  public int run() {
    MCar* car = builder(MCar).build();
    mController.setValue(car);
    int result = mController.getValue().getYear();
    mController.clearValue();
    return result;
  }
}

bind(CProgram).to(IProgram);
